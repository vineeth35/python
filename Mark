marks1 = int(input())
marks2 = int(input())
marks3 = int(input())
average_mark = (marks1 + marks2 + marks3) / 3
if average_mark >= 90:
    grade = 'A'
elif 80 <= average_mark < 90:
    grade = 'B'
elif 70 <= average_mark < 80:
    grade = 'C'
elif 60 <= average_mark < 70:
    grade = 'D'
elif 50 <= average_mark < 60:
    grade = 'E'
else:
    grade = 'F'
print("Average Mark:", average_mark)
print("Grade:", grade)
